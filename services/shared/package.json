{
  "name": "@namecard/serverless-shared",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./auth-index": {
      "types": "./dist/auth-index.d.ts",
      "import": "./dist/auth-index.js",
      "default": "./dist/auth-index.js"
    },
    "./lite-index": {
      "types": "./dist/lite-index.d.ts",
      "import": "./dist/lite-index.js",
      "default": "./dist/lite-index.js"
    },
    "./cards-index": {
      "types": "./dist/cards-index.d.ts",
      "import": "./dist/cards-index.js",
      "default": "./dist/cards-index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
