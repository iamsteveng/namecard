{
  "name": "@namecard/ocr-service",
  "version": "1.0.0",
  "description": "NameCard OCR Service - Lambda Functions",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "sls offline start --stage local",
    "deploy:staging": "sls deploy --stage staging",
    "deploy:production": "sls deploy --stage production",
    "package": "sls package",
    "remove": "sls remove",
    "logs": "sls logs -f extractText --tail",
    "invoke": "sls invoke -f extractText",
    "test": "jest",
    "lint": "eslint *.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-textract": "^3.423.0",
    "@aws-sdk/client-sqs": "^3.423.0",
    "@namecard/serverless-shared": "file:../shared",
    "@namecard/shared": "file:../../packages/shared",
    "aws-lambda": "^1.0.7",
    "aws-xray-sdk-core": "^3.9.0",
    "sharp": "^0.32.6"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "jest": "^30.0.4",
    "serverless": "^3.38.0",
    "serverless-esbuild": "1.54.4",
    "serverless-offline": "13.3.0"
  }
}
